package entities;

public class Employee {
	public String name;
	public double grossSalary;
	public double tax;

	public double netSalary() {
		double result = grossSalary - tax;
		return result;
	}
	
	public void increasySalary(double percentage) {
		double liquidSalary = grossSalary - tax;
		double x = percentage / 100;
		
	}
}
